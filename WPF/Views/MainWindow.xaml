                                                <Label Grid.Column="2" Grid.Row="0" Content="{Binding MarkerList[7][8][2][0]}" Style="{StaticResource StyleLabelMarker}"/>
                                                <Label Grid.Column="3" Grid.Row="0" Content="{Binding MarkerList[7][8][3][0]}" Style="{StaticResource StyleLabelMarker}"/>
                                                <Label Grid.Column="0" Grid.Row="1" Content="{Binding MarkerList[7][8][0][1]}" Style="{StaticResource StyleLabelMarker}"/>
                                                <Label Grid.Column="3" Grid.Row="1" Content="{Binding MarkerList[7][8][3][1]}" Style="{StaticResource StyleLabelMarker}"/>
                                                <Label Grid.Column="0" Grid.Row="2" Content="{Binding MarkerList[7][8][0][2]}" Style="{StaticResource StyleLabelMarker}"/>
                                                <Label Grid.Column="1" Grid.Row="2" Content="{Binding MarkerList[7][8][1][2]}" Style="{StaticResource StyleLabelMarker}"/>
                                                <Label Grid.Column="2" Grid.Row="2" Content="{Binding MarkerList[7][8][2][2]}" Style="{StaticResource StyleLabelMarker}"/>
                                            </local:NumbersInnerGridView>
                                        </Button>
                                        <Button Background="{Binding ButtonBackgroundList[8][8]}" Tag="88" Grid.Column="4" Grid.Row="4" Style="{StaticResource StyleButtonNumber}">
                                            <Button.Resources>
                                                <Style TargetType="Border">
                                                    <Setter Property="CornerRadius" Value="0, 0, 3, 0"/>
                                                    <Setter Property="BorderThickness" Value="0"/>
                                                </Style>
                                            </Button.Resources>
                                            <behaviors:Interaction.Triggers>
                                                <behaviors:EventTrigger EventName="PreviewMouseDown">
                                                    <helpers:AdvancedInvokeCommandAction Command="{Binding ButtonSquareDownCommand}" PassEventArgsToCommand="True" CommandParameter="{Binding Tag, RelativeSource={RelativeSource AncestorType=Button}}"/>
                                                </behaviors:EventTrigger>
                                                <behaviors:EventTrigger EventName="PreviewMouseUp">
                                                    <helpers:AdvancedInvokeCommandAction Command="{Binding ButtonSquareUpCommand}" PassEventArgsToCommand="True"/>
                                                </behaviors:EventTrigger>
                                                <behaviors:EventTrigger EventName="MouseEnter">
                                                    <helpers:AdvancedInvokeCommandAction Command="{Binding ButtonSquareMouseEnterCommand}" PassEventArgsToCommand="True" CommandParameter="{Binding Tag, RelativeSource={RelativeSource AncestorType=Button}}"/>
                                                </behaviors:EventTrigger>
                                                <behaviors:EventTrigger EventName="MouseLeave">
                                                    <helpers:AdvancedInvokeCommandAction Command="{Binding ButtonSquareMouseLeaveCommand}" PassEventArgsToCommand="True" CommandParameter="{Binding Tag, RelativeSource={RelativeSource AncestorType=Button}}"/>
                                                </behaviors:EventTrigger>
                                            </behaviors:Interaction.Triggers>
                                            <local:NumbersInnerGridView>
                                                <Label Grid.Column="1" Grid.Row="1" Grid.ColumnSpan="2" Foreground="{Binding NumberColorList[8][8]}" Content="{Binding NumberList[8][8]}" Style="{StaticResource StyleLabelNumber}"/>
                                                <Label Grid.Column="0" Grid.Row="0" Content="{Binding MarkerList[8][8][0][0]}" Style="{StaticResource StyleLabelMarker}"/>
                                                <Label Grid.Column="1" Grid.Row="0" Content="{Binding MarkerList[8][8][1][0]}" Style="{StaticResource StyleLabelMarker}"/>
                                                <Label Grid.Column="2" Grid.Row="0" Content="{Binding MarkerList[8][8][2][0]}" Style="{StaticResource StyleLabelMarker}"/>
                                                <Label Grid.Column="3" Grid.Row="0" Content="{Binding MarkerList[8][8][3][0]}" Style="{StaticResource StyleLabelMarker}"/>
                                                <Label Grid.Column="0" Grid.Row="1" Content="{Binding MarkerList[8][8][0][1]}" Style="{StaticResource StyleLabelMarker}"/>
                                                <Label Grid.Column="3" Grid.Row="1" Content="{Binding MarkerList[8][8][3][1]}" Style="{StaticResource StyleLabelMarker}"/>
                                                <Label Grid.Column="0" Grid.Row="2" Content="{Binding MarkerList[8][8][0][2]}" Style="{StaticResource StyleLabelMarker}"/>
                                                <Label Grid.Column="1" Grid.Row="2" Content="{Binding MarkerList[8][8][1][2]}" Style="{StaticResource StyleLabelMarker}"/>
                                                <Label Grid.Column="2" Grid.Row="2" Content="{Binding MarkerList[8][8][2][2]}" Style="{StaticResource StyleLabelMarker}"/>
                                            </local:NumbersInnerGridView>
                                        </Button>
                                        <Line Grid.Column="0" Grid.Row="1" Grid.ColumnSpan="5" X1="0" Y1="0" X2="1" Y2="0" Style="{StaticResource StyleLineInnermost}"/>
                                        <Line Grid.Column="0" Grid.Row="3" Grid.ColumnSpan="5" X1="0" Y1="0" X2="1" Y2="0" Style="{StaticResource StyleLineInnermost}"/>
                                        <Line Grid.Column="1" Grid.Row="0" Grid.RowSpan="5" X1="0" Y1="0" X2="0" Y2="1" Style="{StaticResource StyleLineInnermost}"/>
                                        <Line Grid.Column="3" Grid.Row="0" Grid.RowSpan="5" X1="0" Y1="0" X2="0" Y2="1" Style="{StaticResource StyleLineInnermost}"/>
                                    </local:NumbersOuterGridView>
                            
                                    <Line Grid.Column="0" Grid.Row="1" Grid.ColumnSpan="5" X1="0" Y1="0" X2="1" Y2="0" Style="{StaticResource StyleLineMiddle}"/>
                                    <Line Grid.Column="0" Grid.Row="3" Grid.ColumnSpan="5" X1="0" Y1="0" X2="1" Y2="0" Style="{StaticResource StyleLineMiddle}"/>
                                    <Line Grid.Column="1" Grid.Row="0" Grid.RowSpan="5" X1="0" Y1="0" X2="0" Y2="1" Style="{StaticResource StyleLineMiddle}"/>
                                    <Line Grid.Column="3" Grid.Row="0" Grid.RowSpan="5" X1="0" Y1="0" X2="0" Y2="1" Style="{StaticResource StyleLineMiddle}"/>
                                </Grid>
                            </Border>
                        </Viewbox>
                    </Grid>
                </Viewbox>
                <StackPanel x:Name="Validation"
                    Visibility="{Binding ValidationVisibility}">
                    <Label
                        Margin="0"
                        Content=""
                        FontFamily="{StaticResource MainSemiBold}"
                        FontSize="22">
                    </Label>
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="auto"/>
                            <ColumnDefinition Width="auto"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="auto"/>
                            <ColumnDefinition Width="auto"/>
                        </Grid.ColumnDefinitions>
                        <Label
                            Grid.Column="2"
                            Margin="0, 10, 0, 10"
                            Padding="80, 0, 80, 0"                         
                            Style="{StaticResource StyleValidateLabel}"
                            Content="{Binding LabelValidate}"
                            Visibility="{Binding LabelValidateVisibility}">
                            <Label.Resources>
                                <Style TargetType="Border">
                                    <Setter Property="CornerRadius" Value="20"/>
                                </Style>
                            </Label.Resources>
                        </Label>
                        <Button
                            Grid.Column="5"
                            Width="{Binding ElementName=ButtonTrash, Path=ActualWidth}"
                            Height="{Binding ElementName=ButtonTrash, Path=ActualHeight}"
                            HorizontalAlignment="Right"
                            Style="{StaticResource StyleTrashButton}"
                            Command="{Binding ButtonSelectNumberOrMarkerCommand}"
                            CommandParameter="X">
                            <Button.Resources>
                                <Style TargetType="Border">
                                    <Setter Property="CornerRadius" Value="5"/>
                                </Style>
                            </Button.Resources>
                            <Image Margin="10" Source="{Binding Source={StaticResource ImageTrash}}"/>
                        </Button>
                        <Border
                            Grid.Column="1"
                            Grid.ColumnSpan="2"
                            Margin="0, 10, 0, 10"
                            Padding="80, 0, 80, 0"
                            BorderBrush="{Binding Source={x:Static sudoku:Colors.BorderDefault}}"
                            BorderThickness="1"
                            CornerRadius="20"/>
                        <Border
                            Grid.Column="4"
                            Grid.ColumnSpan="2"
                            BorderBrush="{Binding Source={x:Static sudoku:Colors.BorderDefault}}"
                            BorderThickness="2"
                            CornerRadius="5"/>
                    </Grid>
                </StackPanel>
                <StackPanel x:Name="SelectNumberOrMarker"
                    Visibility="{Binding SelectNumberOrMarkerVisibility}">
                    <Label
                        Margin="0"
                        Content="{Binding LabelSelectNumberOrMarker}"
                        FontFamily="{StaticResource MainSemiBold}"
                        FontSize="22">
                    </Label>
                    <Border
                        BorderBrush="{Binding Source={x:Static sudoku:Colors.BorderDefault}}"
                        BorderThickness="2"
                        CornerRadius="5">
                        <Grid
                            HorizontalAlignment="Stretch">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="auto"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="auto"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="auto"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="auto"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="auto"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="auto"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="auto"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="auto"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="auto"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <Button Grid.Column="0" Height="{Binding ActualWidth, RelativeSource={RelativeSource Self}}" Content="1" Style="{StaticResource StyleSelectNumberButton}" Command="{Binding ButtonSelectNumberOrMarkerCommand}" CommandParameter="1">
                                <Button.Resources>
                                    <Style TargetType="Border">
                                        <Setter Property="CornerRadius" Value="3, 0, 0, 3"/>
                                    </Style>
                                </Button.Resources>
                            </Button>
                            <Button Grid.Column="2"  Height="{Binding ActualWidth, RelativeSource={RelativeSource Self}}" Content="2" Style="{StaticResource StyleSelectNumberButton}" Command="{Binding ButtonSelectNumberOrMarkerCommand}" CommandParameter="2"></Button>
                            <Button Grid.Column="4"  Height="{Binding ActualWidth, RelativeSource={RelativeSource Self}}" Content="3" Style="{StaticResource StyleSelectNumberButton}" Command="{Binding ButtonSelectNumberOrMarkerCommand}" CommandParameter="3"></Button>
                            <Button Grid.Column="6"  Height="{Binding ActualWidth, RelativeSource={RelativeSource Self}}" Content="4" Style="{StaticResource StyleSelectNumberButton}" Command="{Binding ButtonSelectNumberOrMarkerCommand}" CommandParameter="4"></Button>
                            <Button Grid.Column="8"  Height="{Binding ActualWidth, RelativeSource={RelativeSource Self}}" Content="5" Style="{StaticResource StyleSelectNumberButton}" Command="{Binding ButtonSelectNumberOrMarkerCommand}" CommandParameter="5"></Button>
                            <Button Grid.Column="10" Height="{Binding ActualWidth, RelativeSource={RelativeSource Self}}" Content="6" Style="{StaticResource StyleSelectNumberButton}" Command="{Binding ButtonSelectNumberOrMarkerCommand}" CommandParameter="6"></Button>
                            <Button Grid.Column="12" Height="{Binding ActualWidth, RelativeSource={RelativeSource Self}}" Content="7" Style="{StaticResource StyleSelectNumberButton}" Command="{Binding ButtonSelectNumberOrMarkerCommand}" CommandParameter="7"></Button>
                            <Button Grid.Column="14" Height="{Binding ActualWidth, RelativeSource={RelativeSource Self}}" Content="8" Style="{StaticResource StyleSelectNumberButton}" Command="{Binding ButtonSelectNumberOrMarkerCommand}" CommandParameter="8"></Button>
                            <Button Grid.Column="16" Height="{Binding ActualWidth, RelativeSource={RelativeSource Self}}" Content="9" Style="{StaticResource StyleSelectNumberButton}" Command="{Binding ButtonSelectNumberOrMarkerCommand}" CommandParameter="9"></Button>
                            <Button Grid.Column="18" Height="{Binding ActualWidth, RelativeSource={RelativeSource Self}}" Style="{StaticResource StyleSelectNumberButton}" Command="{Binding ButtonSelectNumberOrMarkerCommand}" CommandParameter="X" x:Name="ButtonTrash">
                                <Image Margin="10" Source="{Binding Source={StaticResource ImageTrash}}"/>
                                <Button.Resources>
                                    <Style TargetType="Border">
                                        <Setter Property="RenderOptions.BitmapScalingMode" Value="HighQuality"/>
                                        <Setter Property="CornerRadius" Value="0, 3, 3, 0"/>
                                    </Style>
                                </Button.Resources>
                            </Button>
                            <Line Grid.Column="1"  X1="0" X2="0" Y1="0" Y2="1" Style="{StaticResource StyleSelectNumberLineInner}"/>
                            <Line Grid.Column="3"  X1="0" X2="0" Y1="0" Y2="1" Style="{StaticResource StyleSelectNumberLineInner}"/>
                            <Line Grid.Column="5"  X1="0" X2="0" Y1="0" Y2="1" Style="{StaticResource StyleSelectNumberLineInner}"/>
                            <Line Grid.Column="7"  X1="0" X2="0" Y1="0" Y2="1" Style="{StaticResource StyleSelectNumberLineInner}"/>
                            <Line Grid.Column="9"  X1="0" X2="0" Y1="0" Y2="1" Style="{StaticResource StyleSelectNumberLineInner}"/>
                            <Line Grid.Column="11" X1="0" X2="0" Y1="0" Y2="1" Style="{StaticResource StyleSelectNumberLineInner}"/>
                            <Line Grid.Column="13" X1="0" X2="0" Y1="0" Y2="1" Style="{StaticResource StyleSelectNumberLineInner}"/>
                            <Line Grid.Column="15" X1="0" X2="0" Y1="0" Y2="1" Style="{StaticResource StyleSelectNumberLineInner}"/>
                            <Line Grid.Column="17" X1="0" X2="0" Y1="0" Y2="1" Style="{StaticResource StyleSelectNumberLineInner}"/>
                        </Grid>
                    </Border>
                </StackPanel>
            </StackPanel>
        </Viewbox>
    </DockPanel>
</Window>
