                                                <Label Grid.Column="3" Grid.Row="1" Content="{Binding MarkerList[0][0][3][1]}" Style="{StaticResource StyleLabelMarker}"/>
                                                <Label Grid.Column="0" Grid.Row="2" Content="{Binding MarkerList[0][0][0][2]}" Style="{StaticResource StyleLabelMarker}"/>
                                                <Label Grid.Column="1" Grid.Row="2" Content="{Binding MarkerList[0][0][1][2]}" Style="{StaticResource StyleLabelMarker}"/>
                                                <Label Grid.Column="2" Grid.Row="2" Content="{Binding MarkerList[0][0][2][2]}" Style="{StaticResource StyleLabelMarker}"/>
                                            </local:NumbersInnerGridView>
                                        </Button>
                                        <Button Background="{Binding ButtonBackgroundList[1][0]}" Tag="10" Grid.Column="2" Grid.Row="0" Style="{StaticResource StyleButtonNumber}">
                                            <behaviors:Interaction.Triggers>
                                                <behaviors:EventTrigger EventName="PreviewMouseDown">
                                                    <helpers:AdvancedInvokeCommandAction Command="{Binding ButtonSquareDownCommand}" PassEventArgsToCommand="True" CommandParameter="{Binding Tag, RelativeSource={RelativeSource AncestorType=Button}}"/>
                                                </behaviors:EventTrigger>
                                                <behaviors:EventTrigger EventName="PreviewMouseUp">
                                                    <helpers:AdvancedInvokeCommandAction Command="{Binding ButtonSquareUpCommand}" PassEventArgsToCommand="True"/>
                                                </behaviors:EventTrigger>
                                                <behaviors:EventTrigger EventName="MouseEnter">
                                                    <helpers:AdvancedInvokeCommandAction Command="{Binding ButtonSquareMouseEnterCommand}" PassEventArgsToCommand="True" CommandParameter="{Binding Tag, RelativeSource={RelativeSource AncestorType=Button}}"/>
                                                </behaviors:EventTrigger>
                                                <behaviors:EventTrigger EventName="MouseLeave">
                                                    <helpers:AdvancedInvokeCommandAction Command="{Binding ButtonSquareMouseLeaveCommand}" PassEventArgsToCommand="True" CommandParameter="{Binding Tag, RelativeSource={RelativeSource AncestorType=Button}}"/>
                                                </behaviors:EventTrigger>
                                            </behaviors:Interaction.Triggers>
                                            <local:NumbersInnerGridView>
                                                <Label Grid.Column="1" Grid.Row="1" Grid.ColumnSpan="2" Foreground="{Binding NumberColorList[1][0]}" Content="{Binding NumberList[1][0]}" Style="{StaticResource StyleLabelNumber}"/>
                                                <Label Grid.Column="0" Grid.Row="0" Content="{Binding MarkerList[1][0][0][0]}" Style="{StaticResource StyleLabelMarker}"/>
                                                <Label Grid.Column="1" Grid.Row="0" Content="{Binding MarkerList[1][0][1][0]}" Style="{StaticResource StyleLabelMarker}"/>
                                                <Label Grid.Column="2" Grid.Row="0" Content="{Binding MarkerList[1][0][2][0]}" Style="{StaticResource StyleLabelMarker}"/>
                                                <Label Grid.Column="3" Grid.Row="0" Content="{Binding MarkerList[1][0][3][0]}" Style="{StaticResource StyleLabelMarker}"/>
                                                <Label Grid.Column="0" Grid.Row="1" Content="{Binding MarkerList[1][0][0][1]}" Style="{StaticResource StyleLabelMarker}"/>
                                                <Label Grid.Column="3" Grid.Row="1" Content="{Binding MarkerList[1][0][3][1]}" Style="{StaticResource StyleLabelMarker}"/>
                                                <Label Grid.Column="0" Grid.Row="2" Content="{Binding MarkerList[1][0][0][2]}" Style="{StaticResource StyleLabelMarker}"/>
