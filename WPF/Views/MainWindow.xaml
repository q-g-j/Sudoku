                                            </local:NumbersInnerGridView>
                                        </Button>
                                        <Button Background="{Binding ButtonBackgroundList[2][0]}" Tag="20" Grid.Column="4" Grid.Row="0" Style="{StaticResource StyleButtonNumber}">
                                            <behaviors:Interaction.Triggers>
                                                <behaviors:EventTrigger EventName="PreviewMouseDown">
                                                    <helpers:AdvancedInvokeCommandAction Command="{Binding ButtonSquareDownCommand}" PassEventArgsToCommand="True" CommandParameter="{Binding Tag, RelativeSource={RelativeSource AncestorType=Button}}"/>
