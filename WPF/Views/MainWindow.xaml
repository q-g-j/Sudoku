                            Background="Transparent"
                            HorizontalAlignment="Center"
                            Margin="0, 0, 0, 0"
                            Visibility="{Binding SelectDifficultyVisibility}"
                            Panel.ZIndex="2">
                            <Grid HorizontalAlignment="Center">
                                <Border
                                    Margin="40, 40, 40, 30"
                                    Background="White"
                                    BorderBrush="{Binding Source={x:Static sudoku:Colors.BorderDefault}}"
                                    BorderThickness="1"
                                    CornerRadius="10">
                                    <Border.Effect>
                                        <DropShadowEffect />
                                    </Border.Effect>
                                </Border>
                                <Border
                                    Margin="40, 40, 40, 30"
                                    BorderBrush="{Binding Source={x:Static sudoku:Colors.BorderDefault}}"
                                    BorderThickness="0"
                                    CornerRadius="10">
                                    <Button
                                        Command="{Binding ButtonDifficultyEasyCommand}"
                                        Content="{x:Static p:Resources.ButtonDifficultyEasy}"
                                        Style="{StaticResource StyleButtonGameDifficulty}">
                                        <Button.Resources>
                                            <Style TargetType="Border">
                                                <Setter Property="CornerRadius" Value="10" />
                                            </Style>
                                        </Button.Resources>
                                    </Button>
                                </Border>
                            </Grid>

                            <Grid HorizontalAlignment="Center">
                                <Border
                                    Margin="40, 0, 40, 30"
                                    Background="White"
                                    BorderBrush="{Binding Source={x:Static sudoku:Colors.BorderDefault}}"
                                    BorderThickness="1"
                                    CornerRadius="10">
                                    <Border.Effect>
                                        <DropShadowEffect />
                                    </Border.Effect>
                                </Border>
                                <Border
                                    Margin="40, 0, 40, 30"
                                    BorderBrush="{Binding Source={x:Static sudoku:Colors.BorderDefault}}"
                                    BorderThickness="0"
                                    CornerRadius="10">
                                    <Button
                                        Command="{Binding ButtonDifficultyMediumCommand}"
                                        Content="{x:Static p:Resources.ButtonDifficultyMedium}"
                                        Style="{StaticResource StyleButtonGameDifficulty}">
                                        <Button.Resources>
                                            <Style TargetType="Border">
                                                <Setter Property="CornerRadius" Value="10" />
                                            </Style>
                                        </Button.Resources>
                                    </Button>
                                </Border>
                            </Grid>

                            <Grid HorizontalAlignment="Center">
                                <Border
                                    Margin="40, 0, 40, 40"
                                    Background="White"
                                    BorderBrush="{Binding Source={x:Static sudoku:Colors.BorderDefault}}"
                                    BorderThickness="1"
                                    CornerRadius="10">
                                    <Border.Effect>
                                        <DropShadowEffect />
                                    </Border.Effect>
                                </Border>
                                <Border
                                    Margin="40, 0, 40, 40"
                                    BorderBrush="{Binding Source={x:Static sudoku:Colors.BorderDefault}}"
                                    BorderThickness="0"
                                    CornerRadius="10">
                                    <Button
                                        Command="{Binding ButtonDifficultyHardCommand}"
                                        Content="{x:Static p:Resources.ButtonDifficultyHard}"
                                        Style="{StaticResource StyleButtonGameDifficulty}">
                                        <Button.Resources>
                                            <Style TargetType="Border">
                                                <Setter Property="CornerRadius" Value="10" />
                                            </Style>
                                        </Button.Resources>
                                    </Button>
                                </Border>
                            </Grid>
                        </StackPanel>
                        <StackPanel x:Name="SingleSolutionWait"
                            Height="50"
                            Background="Transparent"
                            HorizontalAlignment="Center"
                            Margin="0, 0, 0, 0"
                            Visibility="{Binding LabelSingleSolutionWaitVisibility}"
                            Panel.ZIndex="2">
                            <Grid HorizontalAlignment="Center">
                                <Border
                                    Margin="0, 0, 0, 0"
                                    Background="White"
                                    BorderBrush="{Binding Source={x:Static sudoku:Colors.BorderDefault}}"
                                    BorderThickness="1"
                                    CornerRadius="10">
                                    <Border.Effect>
                                        <DropShadowEffect />
                                    </Border.Effect>
                                </Border>
                                <Border
                                    Margin="0, 0, 0, 0"
                                    BorderBrush="{Binding Source={x:Static sudoku:Colors.BorderDefault}}"
                                    BorderThickness="0"
                                    CornerRadius="10">
                                    <Label
                                        Content="{x:Static p:Resources.LabelOverlaySingleSolutionWait}"
                                        Style="{StaticResource StyleLabelSingleSolutionWait}">
                                        <Label.Resources>
                                            <Style TargetType="Border">
                                                <Setter Property="CornerRadius" Value="10" />
                                            </Style>
                                        </Label.Resources>
                                    </Label>
                                </Border>
                            </Grid>
                        </StackPanel>
                        <StackPanel x:Name="Trophy"
                            Width="{Binding TrophyWidth}"
                            Height="{Binding ActualWidth, RelativeSource={RelativeSource Self}}"
                            Panel.ZIndex="2"
                            Visibility="{Binding TrophyVisibility}">
                            <Image RenderOptions.BitmapScalingMode="HighQuality" Source="{Binding Source={StaticResource ImageTrophy}}"/>
                        </StackPanel>
                        <Viewbox x:Name="SudokuViewbox"
                            Width="650" Height="650">
                            <Border
                                Style="{StaticResource StyleBorderOutermost}">
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="auto"/>
                                        <ColumnDefinition Width="auto"/>
                                        <ColumnDefinition Width="auto"/>
                                        <ColumnDefinition Width="auto"/>
                                        <ColumnDefinition Width="auto"/>
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="auto"/>
                                        <RowDefinition Height="auto"/>
                                        <RowDefinition Height="auto"/>
                                        <RowDefinition Height="auto"/>
                                        <RowDefinition Height="auto"/>
                                    </Grid.RowDefinitions>
                                    
                                    <!-- Column 0, Row 0 -->
                                    <local:NumbersOuterGridView Grid.Column="0" Grid.Row="0">
                                        <Button Background="{Binding ButtonBackgroundList[0][0]}" Tag="00" Grid.Column="0" Grid.Row="0" Style="{StaticResource StyleButtonNumber}">
                                            <Button.Resources>
                                                <Style TargetType="Border">
                                                    <Setter Property="CornerRadius" Value="3, 0, 0, 0"/>
