<Window x:Class="Sudoku.Views.MainWindow"
        x:Name="Main"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:behaviors="http://schemas.microsoft.com/xaml/behaviors"
        xmlns:helpers="clr-namespace:Sudoku.Helpers"
        xmlns:sudoku="clr-namespace:Sudoku"
        xmlns:local="clr-namespace:Sudoku.Views"
        xmlns:p="clr-namespace:Sudoku.Properties"
        xmlns:vm="clr-namespace:Sudoku.ViewModels"
        mc:Ignorable="d"
        Width="600"
        MinWidth="400"
        MinHeight="484"
        SizeToContent="Height"
        Title="Sudoku"
        ResizeMode="CanResize">

    <Window.DataContext>
        <vm:MainWindowViewModel/>
    </Window.DataContext>

    <Window.InputBindings>
        <KeyBinding Key="D1" Command="{Binding KeyboardCommand}" CommandParameter="1"/>
        <KeyBinding Key="D2" Command="{Binding KeyboardCommand}" CommandParameter="2"/>
        <KeyBinding Key="D3" Command="{Binding KeyboardCommand}" CommandParameter="3"/>
        <KeyBinding Key="D4" Command="{Binding KeyboardCommand}" CommandParameter="4"/>
        <KeyBinding Key="D5" Command="{Binding KeyboardCommand}" CommandParameter="5"/>
        <KeyBinding Key="D6" Command="{Binding KeyboardCommand}" CommandParameter="6"/>
        <KeyBinding Key="D7" Command="{Binding KeyboardCommand}" CommandParameter="7"/>
        <KeyBinding Key="D8" Command="{Binding KeyboardCommand}" CommandParameter="8"/>
        <KeyBinding Key="D9" Command="{Binding KeyboardCommand}" CommandParameter="9"/>
        
        <KeyBinding Key="NumPad1" Command="{Binding KeyboardCommand}" CommandParameter="1"/>
        <KeyBinding Key="NumPad2" Command="{Binding KeyboardCommand}" CommandParameter="2"/>
        <KeyBinding Key="NumPad3" Command="{Binding KeyboardCommand}" CommandParameter="3"/>
        <KeyBinding Key="NumPad4" Command="{Binding KeyboardCommand}" CommandParameter="4"/>
        <KeyBinding Key="NumPad5" Command="{Binding KeyboardCommand}" CommandParameter="5"/>
        <KeyBinding Key="NumPad6" Command="{Binding KeyboardCommand}" CommandParameter="6"/>
        <KeyBinding Key="NumPad7" Command="{Binding KeyboardCommand}" CommandParameter="7"/>
        <KeyBinding Key="NumPad8" Command="{Binding KeyboardCommand}" CommandParameter="8"/>
        <KeyBinding Key="NumPad9" Command="{Binding KeyboardCommand}" CommandParameter="9"/>

        <KeyBinding Gesture="Delete" Command="{Binding KeyboardCommand}" CommandParameter="X"/>
        <KeyBinding Gesture="Backspace" Command="{Binding KeyboardCommand}" CommandParameter="X"/>
    </Window.InputBindings>

    <DockPanel>
        <Menu
            Padding="3"
            FontSize="14"
            FontFamily="{StaticResource Main}"
            DockPanel.Dock="Top">
            <Menu.ItemsPanel>
                <ItemsPanelTemplate>
                    <DockPanel HorizontalAlignment="Left"/>
                </ItemsPanelTemplate>
            </Menu.ItemsPanel>
            <MenuItem Header="{x:Static p:Resources.MenuGame}">
                <MenuItem Header="{x:Static p:Resources.MenuGameNewGame}" Command="{Binding MenuNewGameCommand}"></MenuItem>
                <Separator></Separator>
                <MenuItem Header="{x:Static p:Resources.MenuGameEmptySudoku}" Command="{Binding MenuEmptySudokuCommand}"></MenuItem>
                <MenuItem Header="{x:Static p:Resources.MenuFillAllMarkers}" Command="{Binding MenuFillAllMarkersCommand}" ></MenuItem>
                <MenuItem Header="{x:Static p:Resources.MenuRemoveAllMarkers}" Command="{Binding MenuRemoveAllMarkersCommand}" ></MenuItem>
                <MenuItem Header="{x:Static p:Resources.MenuGameSolveSudoku}" Command="{Binding MenuSolveCommand}" ></MenuItem>
                <MenuItem Header="{x:Static p:Resources.MenuGameSettings}">
                    <MenuItem Header="{x:Static p:Resources.MenuGameSettingsSingleSolution}" IsCheckable="True" IsChecked="{Binding MenuSingleSolutionCheck}" Command="{Binding MenuSettingsSingleSolutionCommand}" ></MenuItem>
                </MenuItem>
                <MenuItem Header="{x:Static p:Resources.MenuGamePrint}" Command="{Binding MenuPrintCommand}" CommandParameter="{Binding ElementName=Main, Mode=OneWay}"></MenuItem>
                <Separator></Separator>
                <MenuItem Header="{x:Static p:Resources.MenuGameSaveSlots}">
                    <MenuItem Header="{x:Static p:Resources.MenuGameSaveSlotsSaveToSlot1}" Command="{Binding MenuSaveToSlotCommand}" CommandParameter="1"></MenuItem>
                    <MenuItem Header="{x:Static p:Resources.MenuGameSaveSlotsSaveToSlot2}" Command="{Binding MenuSaveToSlotCommand}" CommandParameter="2"></MenuItem>
                    <MenuItem Header="{x:Static p:Resources.MenuGameSaveSlotsSaveToSlot3}" Command="{Binding MenuSaveToSlotCommand}" CommandParameter="3"></MenuItem>
                    <MenuItem Header="{x:Static p:Resources.MenuGameSaveSlotsSaveToSlot4}" Command="{Binding MenuSaveToSlotCommand}" CommandParameter="4"></MenuItem>
                    <MenuItem Header="{x:Static p:Resources.MenuGameSaveSlotsSaveToSlot5}" Command="{Binding MenuSaveToSlotCommand}" CommandParameter="5"></MenuItem>
                    <Separator></Separator>
                    <MenuItem Header="{Binding MenuSaveSlotsLoadText[0]}" Command="{Binding MenuLoadFromSlotCommand}" CommandParameter="1"></MenuItem>
                    <MenuItem Header="{Binding MenuSaveSlotsLoadText[1]}" Command="{Binding MenuLoadFromSlotCommand}" CommandParameter="2"></MenuItem>
                    <MenuItem Header="{Binding MenuSaveSlotsLoadText[2]}" Command="{Binding MenuLoadFromSlotCommand}" CommandParameter="3"></MenuItem>
                    <MenuItem Header="{Binding MenuSaveSlotsLoadText[3]}" Command="{Binding MenuLoadFromSlotCommand}" CommandParameter="4"></MenuItem>
                    <MenuItem Header="{Binding MenuSaveSlotsLoadText[4]}" Command="{Binding MenuLoadFromSlotCommand}" CommandParameter="5"></MenuItem>
                    <Separator></Separator>
                    <MenuItem Header="{x:Static p:Resources.MenuGameSaveSlotsDeleteAll}" Command="{Binding MenuDeleteAllSlotsCommand}" CommandParameter="5"></MenuItem>
                </MenuItem>
                <Separator></Separator>
                <MenuItem Header="{x:Static p:Resources.MenuGameQuitProgram}" Command="{Binding MenuQuitCommand}"></MenuItem>
            </MenuItem>
            <!--<MenuItem HorizontalAlignment="Right" Header="{x:Static p:Resources.MenuHelp}">
                <MenuItem Header="{x:Static p:Resources.MenuHelpRules}"></MenuItem>
                <MenuItem Header="{x:Static p:Resources.MenuHelpAbout}"></MenuItem>
            </MenuItem>-->
        </Menu>
        <Viewbox x:Name="MainViewbox">
            <StackPanel
                Margin="20">
                <Viewbox x:Name="SudokuWrapperViewbox">
                    <Grid x:Name="SudokuWrapperGrid">
                        <StackPanel x:Name="SelectDifficulty"
