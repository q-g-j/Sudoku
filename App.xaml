<Application x:Class="Sudoku.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Sudoku"
             StartupUri="Views/MainWindow.xaml">
    <Application.Resources>
        <FontFamily x:Key="Title">pack://application:,,,/Assets/Fonts/BowlbyOne-Regular.otf#Bowlby One</FontFamily>
        <FontFamily x:Key="Main">pack://application:,,,/Assets/Fonts/selawk.ttf#Selawik</FontFamily>
        <FontFamily x:Key="Block">pack://application:,,,/Assets/Fonts/JetBrainsMono-Regular.ttf#JetBrains Mono</FontFamily>
    </Application.Resources>
</Application>
