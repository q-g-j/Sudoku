<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:svgc="http://sharpvectors.codeplex.com/svgc/">

    <!-- ObjectDataProvider: Wraps and creates an object that you can use as a binding source. -->
    <!-- ObjectInstance: Gets or sets the object used as the binding source. -->
    <ObjectDataProvider x:Key="ImageTrash" ObjectInstance="{svgc:SvgImage Source=/Sudoku;component/Assets/Images/trash.svg}"/>
</ResourceDictionary>