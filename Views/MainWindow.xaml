                            <Border
                                Margin="15"
                                BorderThickness="1"
                                CornerRadius="10"
                                Background="White"
                                BorderBrush="Black">
                                <Border.Effect>
                                    <DropShadowEffect/>
                                </Border.Effect>
                            </Border>
                            <Border
                                Margin="15"
                                BorderThickness="1"
                                BorderBrush="Black"
                                CornerRadius="10">
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="auto"/>
                                        <ColumnDefinition Width="auto"/>
                                        <ColumnDefinition Width="auto"/>
                                        <ColumnDefinition Width="auto"/>
                                        <ColumnDefinition Width="auto"/>
                                        <ColumnDefinition Width="auto"/>
                                        <ColumnDefinition Width="auto"/>
                                        <ColumnDefinition Width="auto"/>
                                        <ColumnDefinition Width="auto"/>
                                        <ColumnDefinition Width="auto"/>
                                        <ColumnDefinition Width="auto"/>
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="auto"/>
                                        <RowDefinition Height="auto"/>
                                        <RowDefinition Height="auto"/>
                                        <RowDefinition Height="auto"/>
                                        <RowDefinition Height="auto"/>
                                    </Grid.RowDefinitions>
                                    <Button Grid.Column="0" Grid.Row="0" Content="1" Tag="1" Command="{Binding ButtonSelectMarkerCommand}" CommandParameter="1">
                                        <Button.Resources>
                                            <Style TargetType="Border">
                                                <Setter Property="CornerRadius" Value="10, 0, 0, 0"/>
                                            </Style>
                                        </Button.Resources>
                                    </Button>
                                    <Button Grid.Column="2" Grid.Row="0" Content="2" Tag="2" Command="{Binding ButtonSelectMarkerCommand}" CommandParameter="2"/>
                                    <Button Grid.Column="4" Grid.Row="0" Content="3" Tag="3" Command="{Binding ButtonSelectMarkerCommand}" CommandParameter="3"/>
                                    <Button Grid.Column="6" Grid.Row="0" Content="4" Tag="4" Command="{Binding ButtonSelectMarkerCommand}" CommandParameter="4"/>
                                    <Button Grid.Column="8" Grid.Row="0" Content="5" Tag="5" Command="{Binding ButtonSelectMarkerCommand}" CommandParameter="5">
                                        <Button.Resources>
                                            <Style TargetType="Border">
                                                <Setter Property="CornerRadius" Value="0, 10, 0, 0"/>
                                            </Style>
                                        </Button.Resources>
                                    </Button>
                                    <Button Grid.Column="0" Grid.Row="2" Content="6" Tag="6" Command="{Binding ButtonSelectMarkerCommand}" CommandParameter="6">
                                        <Button.Resources>
                                            <Style TargetType="Border">
                                                <Setter Property="CornerRadius" Value="0, 0, 0, 10"/>
                                            </Style>
                                        </Button.Resources>
                                    </Button>
                                    <Button Grid.Column="2" Grid.Row="2" Content="7" Tag="7" Command="{Binding ButtonSelectMarkerCommand}" CommandParameter="7"/>
                                    <Button Grid.Column="4" Grid.Row="2" Content="8" Tag="8" Command="{Binding ButtonSelectMarkerCommand}" CommandParameter="8"/>
                                    <Button Grid.Column="6" Grid.Row="2" Content="9" Tag="9" Command="{Binding ButtonSelectMarkerCommand}" CommandParameter="9"/>
                                    <Button Grid.Column="8" Grid.Row="2" Command="{Binding ButtonSelectMarkerCommand}" CommandParameter="X">
                                        <Image Source="{StaticResource ImageWasteBin}" Margin="20"/>
                                        <Button.Resources>
                                            <Style TargetType="Border">
                                                <Setter Property="CornerRadius" Value="0, 0, 10, 0"/>
                                            </Style>
                                        </Button.Resources>
                                    </Button>
                                    <Line Grid.Column="0" Grid.Row="1" Grid.ColumnSpan="11" X1="0" Y1="0" X2="1" Y2="0" Style="{StaticResource LineInner}"/>
                                    <Line Grid.Column="1" Grid.Row="0" Grid.RowSpan="5" X1="0" Y1="0" X2="0" Y2="1" Style="{StaticResource LineInner}"/>
                                    <Line Grid.Column="3" Grid.Row="0" Grid.RowSpan="5" X1="0" Y1="0" X2="0" Y2="1" Style="{StaticResource LineInner}"/>
                                    <Line Grid.Column="5" Grid.Row="0" Grid.RowSpan="5" X1="0" Y1="0" X2="0" Y2="1" Style="{StaticResource LineInner}"/>
                                    <Line Grid.Column="7" Grid.Row="0" Grid.RowSpan="5" X1="0" Y1="0" X2="0" Y2="1" Style="{StaticResource LineInner}"/>
                                </Grid>
                            </Border>
                        </Grid>
                    </StackPanel>
                </Grid>

                <StackPanel
                    Orientation="Horizontal"
                    HorizontalAlignment="Center">
                    <Button
                        Margin="20, 20, 20, 0"
                        HorizontalAlignment="Center"
                        Command="{Binding ButtonDifficultyCommand}"
                        Content="{x:Static p:Resources.ButtonNewGame}"
                        Style="{StaticResource StyleButtonGame}"
                        Width="{Binding ButtonDifficultyWidth}">
                        <Button.Resources>
                            <Style TargetType="Border">
                                <Setter Property="Border.CornerRadius" Value="10"/>
                            </Style>
                        </Button.Resources>
                    </Button>
                    <Button
                        Margin="20, 20, 20, 0"
                        Command="{Binding ButtonValidateCommand}"
                        Content="{x:Static p:Resources.ButtonValidate}"
                        Style="{StaticResource StyleButtonGame}"
                        Width="250"
                        Visibility="{Binding ButtonValidateVisibility}">                    
                        <Button.Resources>
                            <Style TargetType="Border">
                                <Setter Property="Border.CornerRadius" Value="10"/>
                            </Style>
                        </Button.Resources>
                    </Button>
                    <Label
                        Margin="20, 20, 20, 0"
                        Content="{Binding LabelValidateText}"
                        FontFamily="{StaticResource MainSemiBold}"
                        FontSize="22"
                        Width="250"
                        HorizontalContentAlignment="Center"
                        Visibility="{Binding LabelValidateVisibility}">
                        <Label.Resources>
                            <Style TargetType="Border">
                                <Setter Property="Border.CornerRadius" Value="10"/>
                            </Style>
                        </Label.Resources>
                    </Label>
                </StackPanel>
            
                <StackPanel
                    Height="266"
                    Background="Transparent"
                    HorizontalAlignment="Center"
                    Margin="0, -774, 0, 0"
                    Visibility="{Binding SelectDifficultyVisibility}">
                    <StackPanel.Resources>
                        <Style x:Key="StyleButtonGame" TargetType="Button">
                            <Setter Property="Width" Value="400"/>
                            <Setter Property="Height" Value="40"/>
                            <Setter Property="Background" Value="#eda374"/>
                            <Setter Property="HorizontalContentAlignment" Value="Center"/>
                            <Setter Property="VerticalContentAlignment" Value="Center"/>
                            <Setter Property="HorizontalAlignment" Value="Center"/>
                            <Setter Property="VerticalAlignment" Value="Center"/>
                            <Setter Property="FontFamily" Value="{StaticResource MainSemiBold}"/>
                            <Setter Property="FontSize" Value="22"/>
                        </Style>
                    </StackPanel.Resources>

                    <Grid HorizontalAlignment="Center">
                        <Border
                            Margin="40, 40, 40, 30"
                            Background="White"
                            BorderBrush="Black"
                            BorderThickness="1"
                            CornerRadius="10">
                            <Border.Effect>
                                <DropShadowEffect />
                            </Border.Effect>
                        </Border>
                        <Border
                            Margin="40, 40, 40, 30"
                            BorderBrush="Black"
                            BorderThickness="0"
                            CornerRadius="10">
                            <Button
                                Command="{Binding ButtonDifficultyEasyCommand}"
                                Content="{x:Static p:Resources.ButtonDifficultyEasy}"
                                Style="{StaticResource StyleButtonGame}">
                                <Button.Resources>
                                    <Style TargetType="Border">
                                        <Setter Property="CornerRadius" Value="10" />
                                    </Style>
                                </Button.Resources>
                            </Button>
                        </Border>
                    </Grid>

                    <Grid HorizontalAlignment="Center">
                        <Border
                            Margin="40, 0, 40, 30"
                            Background="White"
                            BorderBrush="Black"
                            BorderThickness="1"
                            CornerRadius="10">
                            <Border.Effect>
                                <DropShadowEffect />
                            </Border.Effect>
                        </Border>
                        <Border
                            Margin="40, 0, 40, 30"
                            BorderBrush="Black"
                            BorderThickness="0"
                            CornerRadius="10">
                            <Button
                                Command="{Binding ButtonDifficultyMediumCommand}"
                                Content="{x:Static p:Resources.ButtonDifficultyMedium}"
                                Style="{StaticResource StyleButtonGame}">
                                <Button.Resources>
                                    <Style TargetType="Border">
                                        <Setter Property="CornerRadius" Value="10" />
                                    </Style>
                                </Button.Resources>
                            </Button>
                        </Border>
                    </Grid>

                    <Grid HorizontalAlignment="Center">
                        <Border
                            Margin="40, 0, 40, 40"
                            Background="White"
                            BorderBrush="Black"
                            BorderThickness="1"
                            CornerRadius="10">
                            <Border.Effect>
                                <DropShadowEffect />
                            </Border.Effect>
                        </Border>
                        <Border
                            Margin="40, 0, 40, 40"
                            BorderBrush="Black"
                            BorderThickness="0"
                            CornerRadius="10">
                            <Button
                                Command="{Binding ButtonDifficultyHardCommand}"
                                Content="{x:Static p:Resources.ButtonDifficultyHard}"
                                Style="{StaticResource StyleButtonGame}">
                                <Button.Resources>
                                    <Style TargetType="Border">
                                        <Setter Property="CornerRadius" Value="10" />
                                    </Style>
                                </Button.Resources>
                            </Button>
                        </Border>
                    </Grid>
                </StackPanel>

                <StackPanel
                    Height="50"
                    Background="Transparent"
                    HorizontalAlignment="Center"
                    Margin="0, -776, 0, 0"
                    Visibility="{Binding LabelUniqueWaitVisibility}">
                    <StackPanel.Resources>
                        <Style x:Key="StyleLabelUniqueWait" TargetType="Label">
                            <Setter Property="Padding" Value="90, 0, 90, 0"/>
                            <Setter Property="Height" Value="50"/>
                            <Setter Property="BorderThickness" Value="1"/>
                            <Setter Property="Background" Value="LightSkyBlue"/>
                            <Setter Property="HorizontalAlignment" Value="Center"/>
                            <Setter Property="VerticalAlignment" Value="Center"/>
                            <Setter Property="HorizontalContentAlignment" Value="Center"/>
                            <Setter Property="VerticalContentAlignment" Value="Center"/>
                            <Setter Property="FontFamily" Value="{StaticResource MainSemiBold}"/>
                            <Setter Property="FontSize" Value="22"/>
                        </Style>
                    </StackPanel.Resources>

                    <Grid HorizontalAlignment="Center">
                        <Border
                            Margin="0, 0, 0, 0"
                            Background="White"
                            BorderBrush="Black"
                            BorderThickness="1"
                            CornerRadius="10">
                            <Border.Effect>
                                <DropShadowEffect />
                            </Border.Effect>
                        </Border>
                        <Border
                        Margin="0, 0, 0, 0"
                        BorderBrush="Black"
                        BorderThickness="0"
                        CornerRadius="10">
                            <Label
                            Content="{x:Static p:Resources.LabelOverlayUniqueWait}"
                            Style="{StaticResource StyleLabelUniqueWait}">
                                    <Label.Resources>
                                    <Style TargetType="Border">
                                        <Setter Property="CornerRadius" Value="10" />
                                    </Style>
                                </Label.Resources>
                            </Label>
                        </Border>
                    </Grid>
                </StackPanel>
            </StackPanel>
        </Viewbox>
    </DockPanel>
</Window>
