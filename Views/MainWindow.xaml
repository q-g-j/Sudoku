<Window x:Class="Sudoku.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sudoku.Views"
        xmlns:vm="clr-namespace:Sudoku.ViewModels"
        mc:Ignorable="d"
        Title="Sudoku"
        Width="500"
        MinWidth="300"
        MinHeight="250"
        SizeToContent="Height"
        ResizeMode="CanResize">

    <Window.DataContext>
        <vm:MainWindowViewModel/>
    </Window.DataContext>

    <Window.Resources>
        <Style x:Key="NumberButton" TargetType="Button">
            <Setter Property="Margin" Value="0"/>
            <Setter Property="Padding" Value="0"/>
            <Setter Property="Width" Value="70"/>
            <Setter Property="Height" Value="70"/>
            <Setter Property="BorderThickness" Value="0"/>
            <Setter Property="Background" Value="White"/>
            <Setter Property="HorizontalContentAlignment" Value="Stretch"/>
            <Setter Property="VerticalContentAlignment" Value="Stretch"/>
            <Setter Property="HorizontalAlignment" Value="Stretch"/>
            <Setter Property="VerticalAlignment" Value="Stretch"/>
        </Style>
        <Style x:Key="NumberButtonPressed" TargetType="Button">
            <Setter Property="Background" Value="LightBlue"/>
        </Style>
        <Style x:Key="GameButton" TargetType="Button">
            <Setter Property="Padding" Value="0"/>
            <Setter Property="Width" Value="400"/>
            <Setter Property="Height" Value="40"/>
            <Setter Property="BorderThickness" Value="1"/>
            <Setter Property="Background" Value="LightSkyBlue"/>
            <Setter Property="HorizontalAlignment" Value="Center"/>
            <Setter Property="VerticalAlignment" Value="Center"/>
            <Setter Property="FontFamily" Value="{StaticResource MainSemiBold}"/>
            <Setter Property="FontSize" Value="22"/>
        </Style>
        <Style x:Key="Marker" TargetType="Label">
            <Setter Property="Margin" Value="-5"/>
            <Setter Property="Padding" Value="0"/>
            <Setter Property="HorizontalContentAlignment" Value="Center"/>
            <Setter Property="VerticalContentAlignment" Value="Center"/>
            <Setter Property="HorizontalAlignment" Value="Stretch"/>
            <Setter Property="VerticalAlignment" Value="Stretch"/>
            <Setter Property="FontFamily" Value="{StaticResource MainSemiBold}"/>
            <Setter Property="FontSize" Value="12"/>
        </Style>
        <Style x:Key="Number" TargetType="Label">
            <Setter Property="Margin" Value="0, -7, 0, 0"/>
            <Setter Property="Padding" Value="0"/>
            <Setter Property="HorizontalContentAlignment" Value="Center"/>
            <Setter Property="VerticalContentAlignment" Value="Center"/>
            <Setter Property="HorizontalAlignment" Value="Stretch"/>
            <Setter Property="VerticalAlignment" Value="Stretch"/>
            <Setter Property="FontFamily" Value="{StaticResource MainBold}"/>
            <Setter Property="FontSize" Value="37"/>
            <Setter Property="FontWeight" Value="Bold"/>
        </Style>
        <Style x:Key="LineInnermost" TargetType="Line">
            <Setter Property="Stroke" Value="#a6a6a6"/>
            <Setter Property="StrokeThickness" Value="1"/>
            <Setter Property="Stretch" Value="Fill"/>
        </Style>
        <Style x:Key="LineMiddle" TargetType="Line">
            <Setter Property="Stroke" Value="#ff4000"/>
            <Setter Property="StrokeThickness" Value="3"/>
            <Setter Property="Stretch" Value="Fill"/>
        </Style>
        <Style x:Key="BorderOutermost" TargetType="Border">
            <Setter Property="BorderBrush" Value="Blue"/>
            <Setter Property="BorderThickness" Value="6"/>
            <Setter Property="CornerRadius" Value="3"/>
        </Style>
    </Window.Resources>
    
    <Viewbox>
        <StackPanel Margin="20">
            <Viewbox>
                <Grid
                    Margin="0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="6"/>
                        <ColumnDefinition Width="auto"/>
                        <ColumnDefinition Width="auto"/>
                        <ColumnDefinition Width="auto"/>
                        <ColumnDefinition Width="auto"/>
                        <ColumnDefinition Width="auto"/>
                        <ColumnDefinition Width="6"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="6"/>
                        <RowDefinition Height="auto"/>
                        <RowDefinition Height="auto"/>
                        <RowDefinition Height="auto"/>
                        <RowDefinition Height="auto"/>
                        <RowDefinition Height="auto"/>
                        <RowDefinition Height="6"/>
                    </Grid.RowDefinitions>

                    <!-- Column 0, Row 0 -->
                    <local:NumbersOuterGridView Grid.Column="1" Grid.Row="1">
                        <Button Grid.Column="0" Grid.Row="0" Style="{StaticResource NumberButton}">
                            <Button.InputBindings>
                                <MouseBinding MouseAction="LeftClick" Command="{Binding ButtonSquareLeftCommand}" CommandParameter="00"/>
                                <MouseBinding MouseAction="RightClick" Command="{Binding ButtonSquareRightCommand}" CommandParameter="00"/>
                            </Button.InputBindings>
                            <local:NumbersInnerGridView HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                                <Label Grid.Column="0" Grid.Row="0" Content="{Binding MarkersList[0][0][0][0]}" Style="{StaticResource Marker}"/>
                                <Label Grid.Column="1" Grid.Row="0" Content="{Binding MarkersList[0][0][1][0]}" Style="{StaticResource Marker}"/>
                                <Label Grid.Column="2" Grid.Row="0" Content="{Binding MarkersList[0][0][2][0]}" Style="{StaticResource Marker}"/>
                                <Label Grid.Column="0" Grid.Row="1" Content="{Binding MarkersList[0][0][0][1]}" Style="{StaticResource Marker}"/>
                                <Label Grid.Column="1" Grid.Row="1" Foreground="{Binding NumbersColorsList[0][0]}" Content="{Binding NumbersList[0][0]}" Style="{StaticResource Number}"/>
                                <Label Grid.Column="2" Grid.Row="1" Content="{Binding MarkersList[0][0][2][1]}" Style="{StaticResource Marker}"/>
                                <Label Grid.Column="0" Grid.Row="2" Content="{Binding MarkersList[0][0][0][2]}" Style="{StaticResource Marker}"/>
                                <Label Grid.Column="1" Grid.Row="2" Content="{Binding MarkersList[0][0][1][2]}" Style="{StaticResource Marker}"/>
                                <Label Grid.Column="2" Grid.Row="2" Content="{Binding MarkersList[0][0][2][2]}" Style="{StaticResource Marker}"/>
                            </local:NumbersInnerGridView>
                        </Button>
                        <Button Grid.Column="2" Grid.Row="0" Style="{StaticResource NumberButton}">
                            <Button.InputBindings>
                                <MouseBinding MouseAction="LeftClick" Command="{Binding ButtonSquareLeftCommand}" CommandParameter="10"/>
                                <MouseBinding MouseAction="RightClick" Command="{Binding ButtonSquareRightCommand}" CommandParameter="10"/>
                            </Button.InputBindings>
                            <local:NumbersInnerGridView>
                                <Label Grid.Column="0" Grid.Row="0" Content="{Binding MarkersList[1][0][0][0]}" Style="{StaticResource Marker}"/>
                                <Label Grid.Column="1" Grid.Row="0" Content="{Binding MarkersList[1][0][1][0]}" Style="{StaticResource Marker}"/>
                                <Label Grid.Column="2" Grid.Row="0" Content="{Binding MarkersList[1][0][2][0]}" Style="{StaticResource Marker}"/>
