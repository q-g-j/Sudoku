                            <RowDefinition Height="auto"/>
                        </Grid.RowDefinitions>
                        <Button Grid.Column="1" Grid.Row="1" Content="1" Tag="1" Command="{Binding ButtonSelectNumberCommand}" CommandParameter="1"/>
                        <Button Grid.Column="3" Grid.Row="1" Content="2" Tag="2" Command="{Binding ButtonSelectNumberCommand}" CommandParameter="2"/>
                        <Button Grid.Column="5" Grid.Row="1" Content="3" Tag="3" Command="{Binding ButtonSelectNumberCommand}" CommandParameter="3"/>
                        <Button Grid.Column="7" Grid.Row="1" Content="4" Tag="4" Command="{Binding ButtonSelectNumberCommand}" CommandParameter="4"/>
                        <Button Grid.Column="9" Grid.Row="1" Content="5" Tag="5" Command="{Binding ButtonSelectNumberCommand}" CommandParameter="5"/>
                        <Button Grid.Column="1" Grid.Row="3" Content="6" Tag="6" Command="{Binding ButtonSelectNumberCommand}" CommandParameter="6"/>
                        <Button Grid.Column="3" Grid.Row="3" Content="7" Tag="7" Command="{Binding ButtonSelectNumberCommand}" CommandParameter="7"/>
                        <Button Grid.Column="5" Grid.Row="3" Content="8" Tag="8" Command="{Binding ButtonSelectNumberCommand}" CommandParameter="8"/>
                        <Button Grid.Column="7" Grid.Row="3" Content="9" Tag="9" Command="{Binding ButtonSelectNumberCommand}" CommandParameter="9"/>
                        <Button Grid.Column="9" Grid.Row="3" Command="{Binding ButtonSelectNumberCommand}" CommandParameter="X">
                            <Image Source="{StaticResource ImageWasteBin}" Margin="20"/>
                        </Button>
                        <Line Grid.Column="0" Grid.Row="2" Grid.ColumnSpan="11" X1="0" Y1="0" X2="1" Y2="0" Style="{StaticResource LineInner}"/>
                        <Line Grid.Column="2" Grid.Row="0" Grid.RowSpan="5" X1="0" Y1="0" X2="0" Y2="1" Style="{StaticResource LineInner}"/>
                        <Line Grid.Column="4" Grid.Row="0" Grid.RowSpan="5" X1="0" Y1="0" X2="0" Y2="1" Style="{StaticResource LineInner}"/>
                        <Line Grid.Column="6" Grid.Row="0" Grid.RowSpan="5" X1="0" Y1="0" X2="0" Y2="1" Style="{StaticResource LineInner}"/>
                        <Line Grid.Column="8" Grid.Row="0" Grid.RowSpan="5" X1="0" Y1="0" X2="0" Y2="1" Style="{StaticResource LineInner}"/>
                        <Border Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="11" Grid.RowSpan="5" Style="{StaticResource BorderOuter}"/>
                        <Grid.Effect>
                            <DropShadowEffect/>
                        </Grid.Effect>
                    </Grid>
                </StackPanel>
            </Grid>
            
            <Grid
                Panel.ZIndex="1000"
                Margin="0, -420, 0, 0"
                HorizontalAlignment="Center"
                Visibility="{Binding SelectMarkerVisibility}">
                <Grid.Resources>
                    <Style x:Key="Title" TargetType="Label">
                        <Setter Property="Margin" Value="0"/>
                        <Setter Property="Padding" Value="0"/>
                        <Setter Property="HorizontalContentAlignment" Value="Center"/>
                        <Setter Property="VerticalContentAlignment" Value="Center"/>
                        <Setter Property="HorizontalAlignment" Value="Center"/>
                        <Setter Property="FontFamily" Value="{StaticResource MainBold}"/>
                        <Setter Property="FontSize" Value="30"/>
                        <Setter Property="FontWeight" Value="Bold"/>
                        <Setter Property="Background" Value="White"/>
                        <!--<Setter Property="FontFamily" Value="/Assets/Fonts/selawk.ttf#Selawik"/>-->
                    </Style>
                    <Style TargetType="Button">
                        <Setter Property="Margin" Value="0"/>
                        <Setter Property="Padding" Value="0"/>
                        <Setter Property="Width" Value="90"/>
                        <Setter Property="Height" Value="90"/>
                        <Setter Property="BorderThickness" Value="0"/>
                        <Setter Property="Background" Value="White"/>
                        <Setter Property="HorizontalContentAlignment" Value="Center"/>
                        <Setter Property="VerticalContentAlignment" Value="Center"/>
                        <Setter Property="HorizontalAlignment" Value="Stretch"/>
                        <Setter Property="VerticalAlignment" Value="Stretch"/>
                        <Setter Property="FontSize" Value="40"/>
                        <Setter Property="FontWeight" Value="Bold"/>
                        <!--<Setter Property="FontFamily" Value="/Assets/Fonts/selawk.ttf#Selawik"/>-->
                    </Style>
                    <Style x:Key="BorderOuter" TargetType="Border">
                        <Setter Property="BorderBrush" Value="Black"/>
                        <Setter Property="BorderThickness" Value="1"/>
                        <Setter Property="CornerRadius" Value="0"/>
                    </Style>
                    <Style x:Key="LineInner" TargetType="Line">
                        <Setter Property="Stroke" Value="#a6a6a6"/>
                        <Setter Property="StrokeThickness" Value="1"/>
                        <Setter Property="Stretch" Value="Fill"/>
                    </Style>
                </Grid.Resources>
                <StackPanel>
                    <Label Content="Marker setzen" Style="{StaticResource Title}"/>
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="auto"/>
                            <ColumnDefinition Width="auto"/>
                            <ColumnDefinition Width="auto"/>
                            <ColumnDefinition Width="auto"/>
                            <ColumnDefinition Width="auto"/>
                            <ColumnDefinition Width="auto"/>
                            <ColumnDefinition Width="auto"/>
                            <ColumnDefinition Width="auto"/>
                            <ColumnDefinition Width="auto"/>
                            <ColumnDefinition Width="auto"/>
                            <ColumnDefinition Width="auto"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="auto"/>
                        </Grid.RowDefinitions>
                        <Button Grid.Column="1" Grid.Row="1" Content="1" Tag="1" Command="{Binding ButtonSelectMarkerCommand}" CommandParameter="1"/>
                        <Button Grid.Column="3" Grid.Row="1" Content="2" Tag="2" Command="{Binding ButtonSelectMarkerCommand}" CommandParameter="2"/>
                        <Button Grid.Column="5" Grid.Row="1" Content="3" Tag="3" Command="{Binding ButtonSelectMarkerCommand}" CommandParameter="3"/>
                        <Button Grid.Column="7" Grid.Row="1" Content="4" Tag="4" Command="{Binding ButtonSelectMarkerCommand}" CommandParameter="4"/>
                        <Button Grid.Column="9" Grid.Row="1" Content="5" Tag="5" Command="{Binding ButtonSelectMarkerCommand}" CommandParameter="5"/>
                        <Button Grid.Column="1" Grid.Row="3" Content="6" Tag="6" Command="{Binding ButtonSelectMarkerCommand}" CommandParameter="6"/>
                        <Button Grid.Column="3" Grid.Row="3" Content="7" Tag="7" Command="{Binding ButtonSelectMarkerCommand}" CommandParameter="7"/>
                        <Button Grid.Column="5" Grid.Row="3" Content="8" Tag="8" Command="{Binding ButtonSelectMarkerCommand}" CommandParameter="8"/>
                        <Button Grid.Column="7" Grid.Row="3" Content="9" Tag="9" Command="{Binding ButtonSelectMarkerCommand}" CommandParameter="9"/>
                        <Button Grid.Column="9" Grid.Row="3" Command="{Binding ButtonSelectMarkerCommand}" CommandParameter="X">
                            <Image Source="{StaticResource ImageWasteBin}" Margin="20"/>
                        </Button>
                        <Line Grid.Column="0" Grid.Row="2" Grid.ColumnSpan="11" X1="0" Y1="0" X2="1" Y2="0" Style="{StaticResource LineInner}"/>
                        <Line Grid.Column="2" Grid.Row="0" Grid.RowSpan="5" X1="0" Y1="0" X2="0" Y2="1" Style="{StaticResource LineInner}"/>
                        <Line Grid.Column="4" Grid.Row="0" Grid.RowSpan="5" X1="0" Y1="0" X2="0" Y2="1" Style="{StaticResource LineInner}"/>
                        <Line Grid.Column="6" Grid.Row="0" Grid.RowSpan="5" X1="0" Y1="0" X2="0" Y2="1" Style="{StaticResource LineInner}"/>
                        <Line Grid.Column="8" Grid.Row="0" Grid.RowSpan="5" X1="0" Y1="0" X2="0" Y2="1" Style="{StaticResource LineInner}"/>
                        <Border Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="11" Grid.RowSpan="5" Style="{StaticResource BorderOuter}"/>
                        <Grid.Effect>
                            <DropShadowEffect/>
                        </Grid.Effect>
                    </Grid>
                </StackPanel>
            </Grid>
            
            <Button
                Command="{Binding ButtonDifficultyCommand}"
                Margin="20, 20, 20, 10"
                Content="{Binding ButtonDifficultyText}"
                Style="{StaticResource GameButton}">
                <Button.Resources>
                    <Style TargetType="Border">
                        <Setter Property="Border.CornerRadius" Value="10"/>
                    </Style>
                </Button.Resources>
            </Button>
            <Button
                Command="{Binding ButtonValidateCommand}"
                Margin="20, 0, 20, 10"
                Content="Überprüfen"
                Style="{StaticResource GameButton}"
                Visibility="{Binding ButtonValidateVisibility}">
                <Button.Resources>
                    <Style TargetType="Border">
                        <Setter Property="Border.CornerRadius" Value="10"/>
                    </Style>
                </Button.Resources>
            </Button>
            <Label
                Margin="20, 0, 20, 0"
                Height="40"
                Content="{Binding LabelValidate}"
                FontFamily="{StaticResource MainSemiBold}"
                FontSize="22"
                HorizontalContentAlignment="Center">
            </Label>
            <StackPanel
                HorizontalAlignment="Center"
                Margin="20, -330, 20, 0"
                Visibility="{Binding SelectDifficultyVisibility}">
                <StackPanel.Resources>
                    <Style x:Key="GameButton" TargetType="Button">
                        <Setter Property="Padding" Value="0"/>
                        <Setter Property="Width" Value="400"/>
                        <Setter Property="Height" Value="40"/>
                        <Setter Property="BorderThickness" Value="1"/>
                        <Setter Property="Background" Value="#eda374"/>
                        <Setter Property="HorizontalAlignment" Value="Center"/>
                        <Setter Property="VerticalAlignment" Value="Center"/>
                        <Setter Property="FontFamily" Value="{StaticResource MainSemiBold}"/>
                        <Setter Property="FontSize" Value="22"/>
                    </Style>
                </StackPanel.Resources>
                <Button
                    Command="{Binding ButtonDifficultyEasyCommand}"
                    Margin="0, 0, 0, 10"
                    Content="Leicht"
                    Style="{StaticResource GameButton}">
                    <Button.Resources>
                        <Style TargetType="Border">
                            <Setter Property="Border.CornerRadius" Value="10"/>
                        </Style>
                    </Button.Resources>
                </Button>
                <Button
                    Command="{Binding ButtonDifficultyMediumCommand}"
                    Margin="0, 0, 0, 10"
                    Content="Mittel"
                    Style="{StaticResource GameButton}">
                    <Button.Resources>
                        <Style TargetType="Border">
                            <Setter Property="Border.CornerRadius" Value="10"/>
                        </Style>
                    </Button.Resources>
                </Button>
                <Button
                    Margin="0, 0, 0, 0"
                    Content="Schwierig"
                    Style="{StaticResource GameButton}">
                    <Button.InputBindings>
                        <MouseBinding
                            MouseAction="LeftClick"
                            Command="{Binding ButtonDifficultyHardCommand}">
                        </MouseBinding>
                    </Button.InputBindings>
                    <Button.Resources>
                        <Style TargetType="Border">
                            <Setter Property="Border.CornerRadius" Value="10"/>
                        </Style>
                    </Button.Resources>
                </Button>
            </StackPanel>
        </StackPanel>
    </Viewbox>
</Window>
