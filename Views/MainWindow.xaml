                                <behaviors:EventTrigger EventName="PreviewMouseDown" SourceObject="{Binding RelativeSource={RelativeSource AncestorType=Button}}">
                                    <b:AdvancedInvokeCommandAction Command="{Binding ButtonSquareDownCommand}" PassEventArgsToCommand="True" CommandParameter="{Binding Tag, RelativeSource={RelativeSource AncestorType=Button}}"/>
                                </behaviors:EventTrigger>
                                <behaviors:EventTrigger EventName="PreviewMouseUp" SourceObject="{Binding RelativeSource={RelativeSource AncestorType=Button}}">
                                    <b:AdvancedInvokeCommandAction Command="{Binding ButtonSquareUpCommand}" PassEventArgsToCommand="True"/>
                                </behaviors:EventTrigger>
                            </behaviors:Interaction.Triggers>
