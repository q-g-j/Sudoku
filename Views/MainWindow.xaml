                                <Label Grid.Column="2" Grid.Row="1" Content="{Binding MarkersList[2][1][2][1]}" Style="{StaticResource Marker}"/>
                                <Label Grid.Column="0" Grid.Row="2" Content="{Binding MarkersList[2][1][0][2]}" Style="{StaticResource Marker}"/>
                                <Label Grid.Column="1" Grid.Row="2" Content="{Binding MarkersList[2][1][1][2]}" Style="{StaticResource Marker}"/>
                                <Label Grid.Column="2" Grid.Row="2" Content="{Binding MarkersList[2][1][2][2]}" Style="{StaticResource Marker}"/>
