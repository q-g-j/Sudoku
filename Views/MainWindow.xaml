                        <Line Grid.Column="8" Grid.Row="0" Grid.RowSpan="5" X1="0" Y1="0" X2="0" Y2="1" Style="{StaticResource LineInner}"/>
                        <Border Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="11" Grid.RowSpan="5" Style="{StaticResource BorderOuter}"/>
                        <Grid.Effect>
                            <DropShadowEffect/>
                        </Grid.Effect>
                    </Grid>
                </StackPanel>
            </Grid>
            
            <Button
                Command="{Binding ButtonDifficultyCommand}"
                Margin="20, 20, 20, 10"
                Content="{Binding ButtonDifficultyText}"
                Style="{StaticResource GameButton}">
                <Button.Resources>
                    <Style TargetType="Border">
                        <Setter Property="Border.CornerRadius" Value="10"/>
                    </Style>
                </Button.Resources>
            </Button>
            <Button
                Command="{Binding ButtonValidateCommand}"
                Margin="20, 0, 20, 10"
                Content="Überprüfen"
                Style="{StaticResource GameButton}"
                Visibility="{Binding ButtonValidateVisibility}">
                <Button.Resources>
                    <Style TargetType="Border">
                        <Setter Property="Border.CornerRadius" Value="10"/>
                    </Style>
                </Button.Resources>
            </Button>
            <Label
                Margin="20, 0, 20, 0"
                Height="40"
                Content="{Binding LabelValidate}"
                FontFamily="{StaticResource MainSemiBold}"
                FontSize="22"
                HorizontalContentAlignment="Center">
            </Label>
            <StackPanel
                HorizontalAlignment="Center"
                Margin="20, -330, 20, 0"
                Visibility="{Binding SelectDifficultyVisibility}">
                <StackPanel.Resources>
                    <Style x:Key="GameButton" TargetType="Button">
                        <Setter Property="Padding" Value="0"/>
                        <Setter Property="Width" Value="400"/>
                        <Setter Property="Height" Value="40"/>
                        <Setter Property="BorderThickness" Value="1"/>
                        <Setter Property="Background" Value="#eda374"/>
                        <Setter Property="HorizontalAlignment" Value="Center"/>
                        <Setter Property="VerticalAlignment" Value="Center"/>
                        <Setter Property="FontFamily" Value="{StaticResource MainSemiBold}"/>
                        <Setter Property="FontSize" Value="22"/>
                    </Style>
                </StackPanel.Resources>
                <Button
                    Command="{Binding ButtonDifficultyEasyCommand}"
                    Margin="0, 0, 0, 10"
                    Content="Leicht"
                    Style="{StaticResource GameButton}">
                    <Button.Resources>
                        <Style TargetType="Border">
                            <Setter Property="Border.CornerRadius" Value="10"/>
                        </Style>
                    </Button.Resources>
                </Button>
                <Button
                    Command="{Binding ButtonDifficultyMediumCommand}"
                    Margin="0, 0, 0, 10"
                    Content="Mittel"
                    Style="{StaticResource GameButton}">
                    <Button.Resources>
                        <Style TargetType="Border">
                            <Setter Property="Border.CornerRadius" Value="10"/>
                        </Style>
                    </Button.Resources>
                </Button>
                <Button
                    Margin="0, 0, 0, 0"
                    Content="Schwierig"
                    Style="{StaticResource GameButton}">
                    <Button.InputBindings>
                        <MouseBinding
                            MouseAction="LeftClick"
                            Command="{Binding ButtonDifficultyHardCommand}">
                        </MouseBinding>
                    </Button.InputBindings>
                    <Button.Resources>
                        <Style TargetType="Border">
                            <Setter Property="Border.CornerRadius" Value="10"/>
                        </Style>
                    </Button.Resources>
                </Button>
            </StackPanel>
        </StackPanel>
    </Viewbox>
</Window>
